# Basic test case from requirements
SELECT MONTHNAME('2008-02-03') AS result1;

# Additional test cases
# All months
SELECT MONTHNAME('2008-01-15') AS jan;
SELECT MONTHNAME('2008-02-15') AS feb;
SELECT MONTHNAME('2008-03-15') AS mar;
SELECT MONTHNAME('2008-04-15') AS apr;
SELECT MONTHNAME('2008-05-15') AS may;
SELECT MONTHNAME('2008-06-15') AS jun;
SELECT MONTHNAME('2008-07-15') AS jul;
SELECT MONTHNAME('2008-08-15') AS aug;
SELECT MONTHNAME('2008-09-15') AS sep;
SELECT MONTHNAME('2008-10-15') AS oct;
SELECT MONTHNAME('2008-11-15') AS nov;
SELECT MONTHNAME('2008-12-15') AS dec;

# Different date types
SELECT MONTHNAME('2008-02-03') AS date_type;
SELECT MONTHNAME('2008-02-03 10:30:45') AS datetime_type;
SELECT MONTHNAME(TIMESTAMP('2008-02-03 10:30:45')) AS timestamp_type;

# NULL handling
SELECT MONTHNAME(NULL) AS null_date;

# Table usage
CREATE TABLE t1(d DATE);
INSERT INTO t1 VALUES ('2008-01-15'), ('2008-06-15'), ('2008-12-15');
SELECT MONTHNAME(d) AS month_name FROM t1;
DROP TABLE t1;

# WHERE clause
CREATE TABLE t1(d DATE);
INSERT INTO t1 VALUES ('2008-01-15'), ('2008-06-15'), ('2008-12-15');
SELECT * FROM t1 WHERE MONTHNAME(d) = 'February';
DROP TABLE t1;

# Different years (should return same month name)
SELECT MONTHNAME('2000-02-03') AS year2000, MONTHNAME('2024-02-03') AS year2024;

