# Basic test case from requirements
SELECT TIMEDIFF('2000:01:01 00:00:00', '2000:01:01 00:00:00.000001') AS result1;

# Additional test cases
# String inputs (datetime format)
SELECT TIMEDIFF('2000:01:01 00:00:00', '2000:01:01 00:00:00.000001') AS microsecond_diff;
SELECT TIMEDIFF('2000:01:01 00:00:00', '2000:01:01 00:00:00') AS zero_diff;
SELECT TIMEDIFF('2000:01:01 00:00:01', '2000:01:01 00:00:00') AS one_second_diff;
SELECT TIMEDIFF('2000:01:01 01:00:00', '2000:01:01 00:00:00') AS one_hour_diff;

# Negative differences
SELECT TIMEDIFF('2000:01:01 00:00:00', '2000:01:01 00:00:01') AS negative_one_second;
SELECT TIMEDIFF('2000:01:01 00:00:00', '2000:01:01 01:00:00') AS negative_one_hour;

# TIME type inputs
SELECT TIMEDIFF('15:30:45', '10:20:30') AS time_diff;
SELECT TIMEDIFF('00:00:00', '00:00:00') AS time_zero;
SELECT TIMEDIFF('23:59:59', '00:00:00') AS time_max_diff;

# DATETIME type inputs
SELECT TIMEDIFF('2000-01-01 15:30:45', '2000-01-01 10:20:30') AS datetime_diff;
SELECT TIMEDIFF('2000-01-01 00:00:00', '2000-01-01 00:00:00') AS datetime_zero;

# Mixed formats
-- @bvt:issue#22985
SELECT TIMEDIFF('2000-01-01 15:30:45', '15:30:45') AS mixed_format1;
SELECT TIMEDIFF('15:30:45', '2000-01-01 15:30:45') AS mixed_format2;
-- @bvt:issue

# With microseconds
SELECT TIMEDIFF('2000-01-01 00:00:00.500000', '2000-01-01 00:00:00.200000') AS with_microseconds;
SELECT TIMEDIFF('2000-01-01 00:00:00.000001', '2000-01-01 00:00:00.000000') AS one_microsecond;

# NULL handling
SELECT TIMEDIFF(NULL, '2000-01-01 00:00:00') AS null_expr1;
SELECT TIMEDIFF('2000-01-01 00:00:00', NULL) AS null_expr2;
SELECT TIMEDIFF(NULL, NULL) AS null_both;

# Table usage
CREATE TABLE t1(dt1 DATETIME, dt2 DATETIME);
INSERT INTO t1 VALUES ('2000-01-01 15:30:45', '2000-01-01 10:20:30'), ('2000-01-01 00:00:00', '2000-01-01 00:00:00');
SELECT TIMEDIFF(dt1, dt2) AS result FROM t1;
DROP TABLE t1;

# WHERE clause
CREATE TABLE t1(dt1 DATETIME, dt2 DATETIME);
INSERT INTO t1 VALUES ('2000-01-01 15:30:45', '2000-01-01 10:20:30'), ('2000-01-01 00:00:00', '2000-01-01 00:00:00');
SELECT * FROM t1 WHERE TIMEDIFF(dt1, dt2) > '02:00:00';
DROP TABLE t1;

# Edge cases
SELECT TIMEDIFF('2000-01-01 23:59:59', '2000-01-01 00:00:00') AS max_day_diff;
SELECT TIMEDIFF('2000-01-02 00:00:00', '2000-01-01 00:00:00') AS day_boundary;
SELECT TIMEDIFF('2000-01-01 00:00:00', '2000-01-02 00:00:00') AS negative_day_boundary;
