# Table usage
CREATE TABLE t1(d DATE);
INSERT INTO t1 VALUES ('2024-01-15'), ('2024-04-15'), ('2024-07-15'), ('2024-12-20');
SELECT d, QUARTER(d) AS quarter FROM t1;
DROP TABLE t1;

# DATETIME table
CREATE TABLE t1(dt DATETIME);
INSERT INTO t1 VALUES ('2024-01-15 10:30:45'), ('2024-04-15 10:30:45'), ('2024-07-15 10:30:45'), ('2024-12-20 10:30:45');
SELECT dt, QUARTER(dt) AS quarter FROM t1;
DROP TABLE t1;

# WHERE clause
CREATE TABLE t1(d DATE);
INSERT INTO t1 VALUES ('2024-01-15'), ('2024-04-15'), ('2024-07-15'), ('2024-12-20');
SELECT * FROM t1 WHERE QUARTER(d) = 4;
DROP TABLE t1;

# Range check (should return 1-4)
SELECT QUARTER('2024-01-01') AS q1, QUARTER('2024-04-01') AS q2, QUARTER('2024-07-01') AS q3, QUARTER('2024-10-01') AS q4;

