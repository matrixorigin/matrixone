# Basic test case from requirements
SELECT INSERT('Hello World', 7, 5, 'MySQL') AS result1;

# Additional test cases
SELECT INSERT('Hello World', 1, 5, 'Hi') AS replace_beginning;
SELECT INSERT('Hello', 6, 1, ' World') AS insert_at_end;
SELECT INSERT('Hello World', 6, 0, 'MySQL ') AS insert_without_remove;
SELECT INSERT('Hello World', 0, 5, 'MySQL') AS invalid_position;
SELECT INSERT('Hello', 10, 5, 'MySQL') AS position_exceeds_length;
SELECT INSERT('Hello', 2, 100, 'MySQL') AS length_exceeds_string;

# NULL handling
SELECT INSERT('Hello World', NULL, 5, 'MySQL') AS null_position;
SELECT INSERT('Hello World', 7, NULL, 'MySQL') AS null_length;
SELECT INSERT('Hello World', 7, 5, NULL) AS null_newstr;
SELECT INSERT(NULL, 7, 5, 'MySQL') AS null_string;

# Edge cases
SELECT INSERT('', 1, 0, 'Hello') AS empty_string;
SELECT INSERT('Hello', 1, 0, '') AS empty_newstr;
SELECT INSERT('Hello World', 1, 11, 'MySQL') AS replace_all;

# Table usage
CREATE TABLE t1(str VARCHAR(50), pos INT, len INT, newstr VARCHAR(50));
INSERT INTO t1 VALUES ('Hello World', 7, 5, 'MySQL'), ('MatrixOne', 1, 6, 'MySQL'), ('Test', 5, 0, ' Result');
SELECT str, pos, len, newstr, INSERT(str, pos, len, newstr) AS result FROM t1;
DROP TABLE t1;

# WHERE clause
CREATE TABLE t1(str VARCHAR(50), pos INT, len INT, newstr VARCHAR(50));
INSERT INTO t1 VALUES ('Hello World', 7, 5, 'MySQL'), ('MatrixOne', 1, 6, 'MySQL'), ('Test', 5, 0, ' Result');
SELECT * FROM t1 WHERE INSERT(str, pos, len, newstr) = 'Hello MySQL';
DROP TABLE t1;

