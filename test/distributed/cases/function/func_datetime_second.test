# Basic test case from requirements
SELECT SECOND('15:30:45') AS result1;

# Additional test cases
# TIME type
SELECT SECOND('15:30:45') AS time_with_seconds;
SELECT SECOND('00:00:00') AS zero_seconds;
SELECT SECOND('23:59:59') AS max_seconds;
SELECT SECOND('12:34:56') AS various_seconds;

# DATETIME type
SELECT SECOND('2024-12-20 15:30:45') AS datetime_with_seconds;
SELECT SECOND('2024-12-20 00:00:00') AS datetime_zero;
SELECT SECOND('2024-12-20 23:59:59') AS datetime_max;

# TIMESTAMP type
SELECT SECOND(TIMESTAMP('2024-12-20 15:30:45')) AS timestamp_with_seconds;

# NULL handling
SELECT SECOND(NULL) AS null_input;

# Table usage
CREATE TABLE t1(t TIME);
INSERT INTO t1 VALUES ('15:30:45'), ('00:00:00'), ('23:59:59'), ('12:34:56');
SELECT t, SECOND(t) AS second FROM t1;
DROP TABLE t1;

# DATETIME table
CREATE TABLE t1(dt DATETIME);
INSERT INTO t1 VALUES ('2024-12-20 15:30:45'), ('2024-12-20 00:00:00'), ('2024-12-20 23:59:59');
SELECT dt, SECOND(dt) AS second FROM t1;
DROP TABLE t1;

# WHERE clause
CREATE TABLE t1(t TIME);
INSERT INTO t1 VALUES ('15:30:45'), ('00:00:00'), ('23:59:59'), ('12:34:56');
SELECT * FROM t1 WHERE SECOND(t) > 30;
DROP TABLE t1;

# Range check (should return 0-59)
SELECT SECOND('15:30:45') AS result1, SECOND('15:30:00') AS result2, SECOND('15:30:59') AS result3;

