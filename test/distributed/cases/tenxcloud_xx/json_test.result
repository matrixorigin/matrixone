id    details
1    {"name": "Laptop"}
UPDATE products SET details = JSON_INSERT(details, '$.price', 999.99);
SELECT * FROM products;
id    details
1    {"name": "Laptop", "price": "999.99"}
INSERT INTO products (details) VALUES ('{"product_info": {"brand": "ABC", "features": []}}');
