#RLIKE
SELECT 
'Corn' RLIKE 'Corn' AS 'Corn',
'Acorn' RLIKE 'Corn' AS 'Acorn',
'Corner' RLIKE 'Corn' AS 'Corner',
'Cheese' RLIKE 'Corn' AS 'Cheese';
SELECT 
'Corn' RLIKE '^Co' AS 'Corn',
'Acorn' RLIKE '^Co' AS 'Acorn',
'Cheese' RLIKE '^Co' AS 'Cheese';
SELECT 
'Corn' RLIKE 'rn$' AS 'Corn',
'Acorn' RLIKE 'rn$' AS 'Acorn',
'Cheese' RLIKE 'rn$' AS 'Cheese'; 
SELECT 
'Corn' RLIKE '.' AS 'Corn',
'Cheese' RLIKE '.' AS 'Cheese';
SELECT 
'Corn' RLIKE '^C.rn$' AS 'Corn', 
'Crn' RLIKE '^C.rn$' AS 'Crn';
SELECT 
'Tweet' RLIKE '^Tw..t$' AS 'Tweet', 
'Tweat' RLIKE '^Tw..t$' AS 'Tweat', 
'Tweet' RLIKE '^Tw.t$' AS 'Tweet', 
'Twit' RLIKE '^Tw..t$' AS 'Twit';
SELECT 
'Tweet' RLIKE '^Tw.{2}t$' AS 'Tweet', 
'Tweat' RLIKE '^Tw.{2}t$' AS 'Tweat', 
'Tweet' RLIKE '^Tw.{1}t$' AS 'Tweet', 
'Twit' RLIKE '^Tw.{2}t$' AS 'Twit';
SELECT 
'Twet' RLIKE '^Twe*t$' AS 'Twet',
'Tweet' RLIKE '^Twe*t$' AS 'Tweet',
'Tweeet' RLIKE'^Twe*t$' AS 'Tweeet',
'Twt' RLIKE '^Twe*t$' AS 'Twt',
'Twit' RLIKE '^Twe*t$' AS 'Twit',
'Twiet' RLIKE '^Twe*t$' AS 'Twiet',
'Tweit' RLIKE '^Twe*t$' AS 'Tweit';
SELECT 
'Twet' RLIKE '^Twe+t$' AS 'Twet',
'Tweet' RLIKE '^Twe+t$' AS 'Tweet',
'Tweeet' RLIKE '^Twe+t$' AS 'Tweeet',
'Twt' RLIKE '^Twe+t$' AS 'Twt',
'Twit' RLIKE '^Twe+t$' AS 'Twit',
'Twiet' RLIKE '^Twe+t$' AS 'Twiet',
'Tweit' RLIKE '^Twe+t$' AS 'Tweit';
SELECT 
'Twet' RLIKE '^Twe?t$' AS 'Twet',
'Tweet' RLIKE '^Twe?t$' AS 'Tweet',
'Tweeet' RLIKE '^Twe?t$' AS 'Tweeet',
'Twt' RLIKE '^Twe?t$' AS 'Twt',
'Twit' RLIKE '^Twe?t$' AS 'Twit',
'Twiet' RLIKE '^Twe?t$' AS 'Twiet',
'Tweit' RLIKE '^Twe?t$' AS 'Tweit';
SELECT 
'Tweet' RLIKE 'Tw|et' AS 'Tweet',
'For Let' RLIKE 'Tw|et' AS 'For Let',
'Banana' RLIKE 'Tw|et' AS 'Banana';
SELECT 
'Cat' RLIKE 'Cat|Dog' AS 'Cat',
'Dog' RLIKE 'Cat|Dog' AS 'Dog',
'Doggone' RLIKE 'Cat|Dog' AS 'Doggone',
'Banana' RLIKE 'Cat|Dog' AS 'Banana';
SELECT 
'Banana' RLIKE '(an)*' AS 'Banana',
'Land' RLIKE '(an)*' AS 'Land',
'Cheese' RLIKE '(an)*' AS 'Cheese';
SELECT 
'Banana' RLIKE '^B(an)*d$' AS 'Banana',
'Band' RLIKE '^B(an)*d$' AS 'Band',
'Bald' RLIKE '^B(an)*d$' AS 'Bald',
'Bad' RLIKE '^B(an)*d$' AS 'Bad';
SELECT 
'Tweeet' RLIKE 'e{3}' AS 'Tweeet',
'Tweet' RLIKE 'e{3}' AS 'Tweet';
SELECT 
'Tweet 123' RLIKE '[0-9]' AS 'Tweet123',
'Tweet ABC' RLIKE '[0-9]' AS 'TweetABC';
SELECT 
'Tweet 123' RLIKE '[A-Z]' AS 'Tweet123',
'ABC' RLIKE '[A-Z]' AS 'ABC',
'123' RLIKE '[A-Z]' AS '123';
SELECT 
'123' RLIKE '[1-3]' AS '123',
'012' RLIKE '[1-3]' AS '012',
'045' RLIKE '[1-3]' AS '045';
SELECT 
'123' RLIKE '[^1-3]' AS '123',
'012' RLIKE '[^1-3]' AS '012',
'045' RLIKE '[^1-3]' AS '045';