drop table if exists t1;
create table t1 (a int, b int);
prepare stmt1 from 'select * from t1 where a > ?';
insert into t1 values (1, 11), (2, 22), (3, 33);
set @a_var = 1;
execute stmt1 using @a_var;
a	b
2	22
3	33
set @a_var = 2;
execute stmt1 using @a_var;
a	b
3	33
insert into t1 values (4, 44);
execute stmt1 using @a_var;
a	b
3	33
4	44
delete from t1 where a > 3;
execute stmt1 using @a_var;
a	b
3	33
deallocate prepare stmt1;
execute stmt1 using @a_var;
prepare statement 'stmt1' does not exist

prepare stmt1 from 'update t1 set a=999 where b = ?';
set @b_var = 33;
execute stmt1 using @b_var;
select * from t1;
a	b
1	11
2	22
999	33
deallocate prepare stmt1;

prepare stmt1 from 'delete from t1 where b = ?';
execute stmt1 using @b_var;
select * from t1;
a	b
1	11
2	22
deallocate prepare stmt1;

prepare stmt1 from "insert into t1 values (?, ?), (?, 99)";
set @a_var=5;
set @b_var=55;
set @c_var=9;
execute stmt1 using @a_var, @b_var, @c_var;
select * from t1;
a	b
1	11
2	22
5	55
9	99
deallocate prepare stmt1;

drop table t1;

